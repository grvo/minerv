    "apollo-upload-client": "^15.0.0",
    "apollo3-cache-persist": "^0.14.1",
    "autosize": "^5.0.1",
    "axios": "^0.24.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-lodash": "^3.3.4",
    "bootstrap": "^4.6.2",
    "browserslist": "^4.21.3",
    "cache-loader": "^4.1.0",
    "canvas-confetti": "^1.4.0",
